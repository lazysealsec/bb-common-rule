    },

    { "action": "allow", "remote-hosts": [ "github.com", "raw.githubusercontent.com" ], "ports": "22,443", "direction": "outgoing" },

    { "action": "allow", "remote-hosts": [ "*.npmjs.org" ], "ports": "443", "direction": "outgoing" },
    { "action": "allow", "remote-hosts": [ "*.pypi.org", "*.pythonhosted.org" ], "ports": "443", "direction": "outgoing" },

    { "action": "allow", "remote": "local-net", "direction": "both" },

    { "action": "allow", "protocol": "icmp", "direction": "incoming", "remote": "any" },

    { "action": "deny", "process": "any", "direction": "both", "priority": "last", "notes": "默认拒绝其余所有流量" }
