    { "action": "allow", "process": "identifier.APPLE/com.apple.trustd", "direction": "outgoing" },

    { "action": "allow", "process": "identifier.MLZF7K7B5R/at.obdev.littlesnitch.networking", "direction": "outgoing" },

    { "action": "allow", "process": "identifier.EQHXZ8M8AV/com.google.Chrome",      "direction": "outgoing" },
